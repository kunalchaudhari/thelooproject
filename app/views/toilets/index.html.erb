<p id="notice"><%= notice %></p>

<h1>Listing Toilets</h1>

<table>
  <thead>
    <tr>
      <th>Lat</th>
      <th>Lng</th>
      <th>Name</th>
      <th>Description</th>
      <th>Open hours</th>
      <th>Closing hours</th>
      <th>Open closing note</th>
      <th>Managed by</th>
      <th>Type</th>
      <th>Genders</th>
      <th>No of toilets</th>
      <th>No of bathrooms</th>
      <th>No of urinals</th>
      <th>Paid</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @toilets.each do |toilet| %>
      <tr>
        <td><%= toilet.lat %></td>
        <td><%= toilet.lng %></td>
        <td><%= toilet.name %></td>
        <td><%= toilet.description %></td>
        <td><%= toilet.open_hours %></td>
        <td><%= toilet.closing_hours %></td>
        <td><%= toilet.open_closing_note %></td>
        <td><%= toilet.managed_by %></td>
        <td><%= toilet.type %></td>
        <td><%= toilet.genders %></td>
        <td><%= toilet.no_of_toilets %></td>
        <td><%= toilet.no_of_bathrooms %></td>
        <td><%= toilet.no_of_urinals %></td>
        <td><%= toilet.paid %></td>
        <td><%= link_to 'Show', toilet %></td>
        <td><%= link_to 'Edit', edit_toilet_path(toilet) %></td>
        <td><%= link_to 'Destroy', toilet, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Toilet', new_toilet_path %>
